{
  "name": "dnd",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://hylle.github.io/stooge-dnd",
  "dependencies": {
    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-sortable-hoc": "^0.8.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build-css": "node-sass-chokidar src/ -o src/ --include-path assets/scss/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive --include-path assets/scss/",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "node-fetch-json": "^0.0.4",
    "node-sass-chokidar": "^1.3.3",
    "prettier": "1.13.7",
    "queue": "^4.4.2"
  }
}
