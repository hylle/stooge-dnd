    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "cldr-core": "^33.0.0",
    "expect.js": "*",
    "http-server": "^0.10.0",
    "mocha": "^4.0.1",
    "standard": "^10.0.3",
    "uglify-js": "^3.1.9"
  },
  "optionalDependencies": {
    "minimist": "^1.2.0"
  }
}
